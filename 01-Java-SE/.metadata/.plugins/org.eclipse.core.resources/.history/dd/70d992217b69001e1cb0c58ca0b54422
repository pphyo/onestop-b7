package com.jdc.inherit;

public class Duck extends Animal {
	
	public String name = "Kalayar";
	public int value = 30;
	
	public Duck() {
		super("Pet");
	}
	
	public void eat(String meat) {
		System.out.println("Duck eats %s".formatted(meat));
	}
	
	@Override
	public static void shift() {
		
	}

}








